<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SEGP7.TabbedNavigation"
             xmlns:pages="clr-namespace:SEGP7.Pages"
             Title="Tabs">
    <TabBar x:Name="Tabs">
        <Tab Title="Profile" Icon="accountico.png">
            <ShellContent ContentTemplate="{DataTemplate pages:ProfilePage}"/>
        </Tab>
        <Tab Title="Notifications" Icon="notifications.png">
            <ShellContent ContentTemplate="{DataTemplate pages:NotificationsPage}"/>
        </Tab>
        <Tab Title="Destress" Icon="heart.png">
            <ShellContent ContentTemplate="{DataTemplate pages:DestressPage}"/>
        </Tab>
        <Tab Title="Checklist" Icon="checklist.png">
            <ShellContent ContentTemplate="{DataTemplate pages:ChecklistPage}"/>
        </Tab>
        <Tab Title="Journal" Icon="journal.png">
            <ShellContent ContentTemplate="{DataTemplate pages:JournalPage}"/>
        </Tab>
    </TabBar>
</Shell>